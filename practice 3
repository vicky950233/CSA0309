#include <stdio.h>
int main() {
    int a[10], b[10], c[20], n, m, i, j = 0;
    scanf("%d%d", &n, &m);
    for (i = 0; i < n; i++) scanf("%d", &a[i]);
    for (i = 0; i < m; i++) scanf("%d", &b[i]);
    for (i = 0; i < n; i++) c[j++] = a[i];
    for (i = 0; i < m; i++) c[j++] = b[i];
    for (i = 0; i < j; i++) printf("%d ", c[i]);
}
