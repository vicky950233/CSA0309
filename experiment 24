#include <stdio.h>
#define INF 9999
#define MAX 10

int main() {
    int n, cost[MAX][MAX], visited[MAX]={0};
    int i, j, u, v, edges=0, min, total=0;

    printf("Enter number of vertices: ");
    scanf("%d",&n);

    printf("Enter cost matrix (9999 for no edge):\n");
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            scanf("%d",&cost[i][j]);

    visited[0]=1; // start from vertex 0

    printf("Edges in MST:\n");
    while(edges < n-1) {
        min=INF;
        for(i=0;i<n;i++)
            if(visited[i])
                for(j=0;j<n;j++)
                    if(!visited[j] && cost[i][j]<min) {
                        min=cost[i][j];
                        u=i; v=j;
                    }
        printf("%d - %d  cost=%d\n",u,v,min);
        total+=min;
        visited[v]=1;
        edges++;
    }
    printf("Minimum cost = %d\n", total);
    return 0;
}
