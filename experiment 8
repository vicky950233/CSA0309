#include <stdio.h>
int main() {
    int a[50], n, i, key, flag=0;
    printf("Enter size: ");
    scanf("%d",&n);
    printf("Enter elements: ");
    for(i=0;i<n;i++) scanf("%d",&a[i]);
    printf("Enter number to search: ");
    scanf("%d",&key);
    for(i=0;i<n;i++){
        if(a[i]==key){ 
            printf("Found at position %d",i+1);
            flag=1; break;
        }
    }
    if(!flag) printf("Not Found");
    return 0;
}
