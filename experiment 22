#include <stdio.h>

int adj[10][10], visited[10], n;

void DFS(int v) {
    printf("%d ", v);
    visited[v] = 1;
    for(int i=0;i<n;i++) {
        if(adj[v][i] && !visited[i])
            DFS(i);
    }
}

int main() {
    int i,j,start;
    printf("Enter number of vertices: ");
    scanf("%d",&n);

    printf("Enter adjacency matrix:\n");
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            scanf("%d",&adj[i][j]);

    printf("Enter starting vertex: ");
    scanf("%d",&start);

    for(i=0;i<n;i++) visited[i]=0;

    printf("DFS Traversal: ");
    DFS(start);
    return 0;
}
