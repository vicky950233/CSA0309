#include <stdio.h>
#define V 5
#define INF 999
int minD(int d[],int v[],int n){int m=INF,i,idx=-1;for(i=0;i<n;i++)if(!v[i]&&d[i]<m){m=d[i];idx=i;}return idx;}
void dijkstra(int g[V][V],int s){
 int d[V],v[V]={0},i,j;
 for(i=0;i<V;i++)d[i]=INF; d[s]=0;
 for(i=0;i<V-1;i++){int u=minD(d,v,V);v[u]=1;for(j=0;j<V;j++)if(!v[j]&&g[u][j]&&d[u]+g[u][j]<d[j])d[j]=d[u]+g[u][j];}
 for(i=0;i<V;i++)printf("%d ",d[i]);
}
int main(){int g[V][V]={{0,9,0,0,0},{9,0,10,5,0},{0,10,0,0,1},{0,5,0,0,2},{0,0,1,2,0}};dijkstra(g,0);}
