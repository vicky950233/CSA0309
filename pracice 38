#include <stdio.h>
int main(){
 int a[10][10],r,c,i,j;scanf("%d%d",&r,&c);
 for(i=0;i<r;i++)for(j=0;j<c;j++)scanf("%d",&a[i][j]);
 for(i=0;i<r;i++){int sum=0;for(j=0;j<c;j++)sum+=a[i][j];printf("Row%d=%d\n",i,sum);}
 for(j=0;j<c;j++){int sum=0;for(i=0;i<r;i++)sum+=a[i][j];printf("Col%d=%d\n",j,sum);}
}
