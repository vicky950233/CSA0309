#include <stdio.h>
#define V 4
#define INF 999
int min(int a,int b){return a<b?a:b;}
int tsp(int g[V][V],int mask,int pos,int dp[V][1<<V]){
 if(mask==(1<<V)-1)return g[pos][0];
 if(dp[pos][mask]!=-1)return dp[pos][mask];
 int ans=INF;
 for(int city=0;city<V;city++)
  if(!(mask&(1<<city)))ans=min(ans,g[pos][city]+tsp(g,mask|(1<<city),city,dp));
 return dp[pos][mask]=ans;
}
int main(){
 int g[V][V]={{0,10,15,20},{10,0,35,25},{15,35,0,30},{20,25,30,0}},dp[V][1<<V];
 for(int i=0;i<V;i++)for(int j=0;j<(1<<V);j++)dp[i][j]=-1;
 printf("%d",tsp(g,1,0,dp));
}
