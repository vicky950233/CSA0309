#include <stdio.h>
#include <stdlib.h>
struct Node{int d;struct Node*l,*r;};
struct Node*insert(struct Node*r,int v){if(!r){r=malloc(sizeof(*r));r->d=v;r->l=r->r=NULL;return r;}
 if(v<r->d)r->l=insert(r->l,v);else r->r=insert(r->r,v);return r;}
void kthMin(struct Node*r,int *c,int k){
    if(r){kthMin(r->l,c,k);(*c)++;if(*c==k){printf("%d",r->d);return;}kthMin(r->r,c,k);}
}
int main(){int n,x,k;scanf("%d",&n);struct Node*r=NULL;
 for(int i=0;i<n;i++){scanf("%d",&x);r=insert(r,x);}scanf("%d",&k);
 int c=0;kthMin(r,&c,k);
}
